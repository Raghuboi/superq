{
  "name": "superq",
  "version": "1.0.0",
  "description": "Queue-based hash service with LRU cache",
  "type": "module",
  "author": "Raghunath Prabhakar",
  "license": "MIT",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "test": "LOG_LEVEL=warn NODE_ENV=test node --import tsx --test --test-timeout=30000 --experimental-test-coverage --test-reporter=spec '__tests__/**/*.test.ts'",
    "docker:up": "COMPOSE_BAKE=true docker compose up --build -d",
    "docker:watch": "COMPOSE_BAKE=true docker compose up --build --watch",
    "docker:down": "docker compose down --remove-orphans",
    "docker:logs": "docker compose logs -f backend",
    "docker:health": "docker compose ps"
  },
  "engines": {
    "node": ">=25.0.0"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@hono/swagger-ui": "^0.5.3",
    "@hono/zod-openapi": "^1.2.1",
    "@t3-oss/env-core": "^0.13.10",
    "hono": "^4.11.7",
    "ioredis": "^5.9.2",
    "pino": "^10.3.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.10.0",
    "eslint": "^9.39.2",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.54.0"
  }
}
